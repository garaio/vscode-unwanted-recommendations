{
  "name": "garaio-vscode-unwanted-recommendations",
  "displayName": "Unwanted Recommendations",
  "description": "Check and warn for extensions which are marked as 'unwanted recommendations'.",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/garaio/vscode-unwanted-recommendations"
  },
  "publisher": "GARAIOAG",
  "author": {
    "email": "fabian.gander@garaio.com",
    "name": "Fabian Gander"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "extensions",
    "extension",
    "unwanted",
    "recommendations",
    "disable",
    "disable extension",
    "unwantedRecommendations",
    "exensions.json"
  ],
  "activationEvents": [
    "onCommand:extensions-unwanted-recommendations.checkPackages",
    "workspaceContains:.vscode/extensions.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extensions-unwanted-recommendations.checkPackages",
        "title": "Check for unwanted recommendations (extensions)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/hjson": "^2.4.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "hjson": "^3.2.2"
  }
}